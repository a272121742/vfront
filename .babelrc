{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "es2015",
    "stage-3",
    "vue"
  ],
  "plugins": [
    "transform-vue-jsx", 
    "transform-runtime", 
    "transform-object-rest-spread"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-3"],
      "plugins": ["transform-vue-jsx", "istanbul"]
    }
  }
}
